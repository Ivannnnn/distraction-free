{
  "name": "Distraction Free",
  "description": "",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["twitterScript.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://*.reddit.com/*"],
      "js": ["redditScript.js"],
      "run_at": "document_end"
    }
  ]
}